config_version=5

[application]

config/name="Space Craft 22"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.png"

[autoload]

Util="*res://Util.gd"
Client="*res://Client.gd"
Procgen="*res://procgen/Procgen.tscn"
Data="*res://data/Data.tscn"
Cheats="*res://Cheats.tscn"
SystemGen="*res://procgen/SystemGen.gd"

[editor]

movie_writer/movie_file="/home/eamonnm/movie.avi"

[global]

contrast=false
sharp=false

